"amixer -D pulse set Master toggle; amixer set Headphone unmute; amixer set Speaker unmute; ~/.status.sh"
    XF86AudioMute

"amixer -D pulse set Master 5%-; ~/.status.sh"
    XF86AudioLowerVolume

"amixer -D pulse set Master 5%+; ~/.status.sh"
    XF86AudioRaiseVolume

"sudo light -s sysfs/backlight/intel_backlight -U 10"
    XF86MonBrightnessDown

"sudo light -s sysfs/backlight/intel_backlight -A 10"
    XF86MonBrightnessUp

"xclip -o -selection primary | xclip -selection clipboard"
    Mod4 + c

"slock"
    Mod4 + Shift + l

"touch ~/.last_workout; ~/.status.sh"
    Mod4 + Shift + w
