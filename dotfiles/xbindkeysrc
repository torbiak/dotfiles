"amixer set Master toggle; amixer set Headphone unmute; amixer set Speaker unmute; statusbar once"
    XF86AudioMute

"amixer set Master 5%-; statusbar once"
    XF86AudioLowerVolume

"amixer set Master 5%+; statusbar once"
    XF86AudioRaiseVolume

"sudo light -s sysfs/backlight/intel_backlight -U 10"
    XF86MonBrightnessDown

"sudo light -s sysfs/backlight/intel_backlight -A 10"
    XF86MonBrightnessUp

"xclip -o -selection primary | xclip -selection clipboard"
    Mod4 + c

"slock"
    Mod4 + Shift + l

"touch ~/.last_workout; statusbar once"
    Mod4 + Shift + w
